<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="5dip" />
            <solid android:color="@color/white"/>
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip android:clipOrientation="horizontal" android:gravity="start">
            <shape>
                <corners android:radius="5dip" />
                <gradient
                    android:startColor="@color/light_blue_A400"
                    android:centerColor="@color/light_blue_A400"
                    android:centerY="0.75"
                    android:endColor="@color/light_blue_A400"
                    android:angle="180"
                    />
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip android:clipOrientation="horizontal" android:gravity="start">
            <shape>
                <corners android:radius="5dip" />
                <gradient
                    android:startColor="@color/orange"
                    android:centerColor="@color/orange"
                    android:centerY="0.75"
                    android:endColor="@color/orange"
                    android:angle="180"
                    />
            </shape>
        </clip>
    </item>
</layer-list>